    scale_fill_manual(values = c("red3", "goldenrod2")) + 
    labs(
      x = "Attractor Strength"
      , y = "Density"
      , title = "Individual Differences in Attractor Strength Estimates"
    ) + 
    facet_grid(emo_item ~ parameter) + 
    my_theme() + 
   theme(legend.position = "none")


