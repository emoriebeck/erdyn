
library(brms)
library(tidyverse)

d <- read_csv("~/Downloads/cpe-AGR-location-data.csv")# %>% 
  # rename(estimate = mean, std.error = se)

m0 <- brm(
  mean | se(se, sigma = TRUE) ~ 1 + (1 | SID),
  , data = d
  , family = gaussian
  , prior = c(prior(exponential(1), class = sd))
  , iter = 2000
  , warmup = 1000
  , cores = 4
  , chains = 4
  , seed = 15
  )

spank <- readxl::read_excel("~/Downloads/spank.xlsx")
